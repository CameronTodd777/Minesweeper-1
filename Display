import javafx.scene.layout.HBox;
import javafx.scene.text.Font;
import javafx.scene.text.Text;

// This will store our display above the main board.
public class Display extends HBox {

    Text tempText;

    Display() {
        tempText = new Text("Here is where score and stuff will go");
        tempText.setFont(Font.font(50));
        getChildren().addAll(tempText);
    }
}
